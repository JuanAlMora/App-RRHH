<body>
    <div class="container h-screen flex overflow-hidden" id="contenedorPrincipal" style="padding-left: 0px;margin-left: 0px;width: 130%;padding-right: 0px;padding-top: 0px;margin-top: 0px;margin-right: -1px;max-width: 100%;">
        <div class="row" style="width: 100%;height: 110px;">
            <div class="col-3 col-sm-1 col-md-1 col-lg-1 col-xl-1 col-xxl-1" style="width: 100.5px;padding-right: 0px;padding-left: 0px;background: #484850;"><img width="95" height="99" src="assets/img/RRHH%20App.png" style="width: 100%;"></div>
            <div class="col" id="tituloPagina" style="padding-left: 0px;background: #484850;padding-right: 0px;margin-right: 0px;width: 120%;">
                <h1 id="tituloCandidatos"><span style="color: rgb(255, 255, 255);">VACANTES</span></h1>
            </div>
        </div>
        <div></div>
        <div class="row" style="padding-bottom: 0px;margin-bottom: 0px;margin-right: 0px;margin-left: 0px;width: 100%;height: 100%;">
            <div class="col-3 col-sm-2 col-md-1 col-lg-1 col-xl-1 col-xxl-1 offset-lg-0" id="barraMenu" style="background: #484850;border-style: none;color: rgb(255,255,255);padding: 0px;height: 100%;min-height: 100%;max-height: 100%;margin-bottom: 0px;padding-bottom: 0px;width: 100.5px;">
                <div><a id="botonSalir-1" href="/menuCandidatos" style="width: auto;color: rgb(255,255,255);"><i class="typcn typcn-arrow-back" style="font-size: 93px;padding-left: 0px;"></i></a></div>
                <div id="logo" style="background: rgba(255,255,255,0);height: auto;width: 100%;">
                    <div id="iconoPerfil" style="padding-left: 6px;margin-top: 300px;margin-bottom: 200px;"><svg xmlns="http://www.w3.org/2000/svg" viewBox="-32 0 512 512" width="1em" height="1em" fill="currentColor" id="perfilCorbata" style="font-size: 49px;">
                            <!--! Font Awesome Free 6.1.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free (Icons: CC BY 4.0, Fonts: SIL OFL 1.1, Code: MIT License) Copyright 2022 Fonticons, Inc. -->
                            <path d="M352 128C352 198.7 294.7 256 224 256C153.3 256 96 198.7 96 128C96 57.31 153.3 0 224 0C294.7 0 352 57.31 352 128zM209.1 359.2L176 304H272L238.9 359.2L272.2 483.1L311.7 321.9C388.9 333.9 448 400.7 448 481.3C448 498.2 434.2 512 417.3 512H30.72C13.75 512 0 498.2 0 481.3C0 400.7 59.09 333.9 136.3 321.9L175.8 483.1L209.1 359.2z"></path>
                        </svg><button class="btn btn-primary" id="botonPerfil" type="button" style="width: 90%;height: 30px;font-size: 12px;">Perfil</button><button class="btn btn-primary" id="botonSalir-2" type="button" style="width: 90%;height: 30px;font-size: 12px;"><a id="linkSalir" href="login.html" rel="prev" style="color: var(--bs-btn-color);text-decoration: underline;">Salir</a></button></div>
                </div>
            </div>
            <div class="col-7 col-sm-8 col-md-9 col-lg-10 col-xl-10 col-xxl-11 offset-lg-3" id="ColumnaPrincipal" style="margin-left: 0px;padding-left: 0px;padding-right: 0px;margin-right: 0px;padding-bottom: 0px;margin-bottom: 0px;">
                <div style="margin-left: 26px;margin-top: 15px;"><a class="btn btn-primary" role="button" href="/nuevaVacante"><svg xmlns="http://www.w3.org/2000/svg" width="1em" height="1em" viewBox="0 0 24 24" fill="none" style="font-size: 28px;padding-left: 0px;margin-left: 10px;">
                            <path d="M2 5H14V7H2V5Z" fill="currentColor"></path>
                            <path d="M2 9H14V11H2V9Z" fill="currentColor"></path>
                            <path d="M10 13H2V15H10V13Z" fill="currentColor"></path>
                            <path d="M16 9H18V13H22V15H18V19H16V15H12V13H16V9Z" fill="currentColor"></path>
                        </svg></a></div>
                <div class="row" style="width: 1400px;margin-left: 15px;margin-top: 15px;">

                    {{#each rows}}
                          {{#if (eq (mod @index 3) 0)}}
                            <div class="row" style="width: 1400px;margin-left: 15px;margin-top: 15px;">
                            {{/if}}

                        <div class="col-md-6 col-xl-3 mb-4">
                            <div class="card shadow border-start-primary py-2">
                                <div class="card-body" style="padding-bottom: 0px;padding-top: 0px;">
                                    <div class="row align-items-center no-gutters">
                                        <div class="col me-2">
                                            <div class="text-uppercase text-primary fw-bold text-xs mb-1"><span style="font-size: 12px;color: var(--bs-primary);">{{nombre}}</span></div>
                                            <div class="text-dark fw-bold h5 mb-0" style="margin-top: -6px;"><span style="font-size: 14px;"></span><span style="font-size: 14px;">{{id}}</span><span style="font-size: 10px;margin-left: 12px;color: var(--bs-gray-600);">{{salario}}</span></div>
                                        </div>
                                        <div class="col-auto"><i class="fas fa-check fa-2x" data-bs-toggle="tooltip" data-bss-tooltip="" style="color: var(--bs-primary);font-size: 28px;margin-top: 4px;" title="{{detalles}}"></i></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {{#if (eq (mod @index 3) 2)}}
                        </div>
                    {{/if}}

                    {{/each}}  


                </div>
            </div>
        </div>
    </div>